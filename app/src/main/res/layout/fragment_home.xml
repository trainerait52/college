<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/ahp_admin"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:drawablePadding="10sp"
        android:gravity="center"
        android:text="Admin"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:drawableTopCompat="@drawable/admi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.099" />

    <TextView
        android:id="@+id/ahp_teacher"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:drawablePadding="10sp"
        android:gravity="center"
        android:text="Teacher"
        android:onClick="teacher"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:drawableTopCompat="@drawable/admi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/ahp_students"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:drawablePadding="10sp"
        android:gravity="center"
        android:text="Student"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:drawableTopCompat="@drawable/studentt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ahp_admin"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/ahp_parents"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:drawablePadding="10sp"
        android:gravity="center"
        android:text="Parents"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:drawableTopCompat="@drawable/parentttt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ahp_teacher"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/ahp_staff"

        android:layout_width="150dp"
        android:layout_height="150dp"
        android:drawablePadding="10sp"
        android:gravity="center"
        android:text="Staff"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:drawableTopCompat="@drawable/staffs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ahp_students"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/ahp_feedback"

        android:layout_width="150dp"
        android:layout_height="150dp"
        android:drawablePadding="10sp"
        android:gravity="center"
        android:text="Feedback"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:drawableTopCompat="@drawable/feed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ahp_parents"
        app:layout_constraintVertical_bias="0.1" />

</androidx.constraintlayout.widget.ConstraintLayout>